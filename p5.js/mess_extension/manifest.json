{
  "manifest_version": 2,
  "name": "Hostel 5-Mess Menu",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["script.js"]
    }
  ],
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "index.html",
    "default_title": "Hostel 5-Mess Menu"
  }
}